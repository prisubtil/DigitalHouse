package main;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		Produto x = new Produto();
		
		System.out.println("Digite : ");
		x.nome = sc.next();
		x.preco = sc.nextDouble();
		x.quantidade = sc.nextInt();

	}

}
